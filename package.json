    "core-js": "^3.27.2",
    "vue": "^3.2.41",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/compiler-sfc": "^3.2.41",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.9.0",
    "node-sass": "^7.0.3",
    "sass": "^1.57.1",
    "sass-loader": "^13.1.0"
