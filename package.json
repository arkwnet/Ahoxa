{
  "name": "ahoxa",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.22.5",
    "vue": "^3.2.34",
    "vue-axios": "^3.4.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.34",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^9.0.1",
    "node-sass": "^6.0.1",
    "sass": "^1.52.1",
    "sass-loader": "^10.2.1"
  },
  "eslintConfig": {
    "root": true,
