    "core-js": "^3.30.2",
    "vue": "^3.2.47",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/compiler-sfc": "^3.3.1",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.1",
    "node-sass": "^8.0.0",
